<% @page_title = "test-app | New Owner" %>
<div class="New Owner">
<h1>Create Owner</h1>
<%= form_for(:owner, :url => {:controller => 'owner', :action => 'create'}) do |f| %>
<p> Name:</br> <%= f.text_field :name%> </p>
<p> Client:</br> <%= f.text_field :client%> </p>
<p> Process:</br> <%= f.text_field :process%> </p>
<%= f.submit :Register %>
<% end %>
<% if @owner.errors.any? %>
<ul class="Signup_Errors">
<% for message_error in @owner.errors.full_messages %>
<li>* <%= message_error %></li>
<% end %>
</ul>
<% end %>