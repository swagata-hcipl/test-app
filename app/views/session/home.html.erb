	
  <h2 class='User_Header'> Welcome, <%= @current_user.try(:name)%> </h2>

<table>
  <tr>
     <th>ID &nbsp; &nbsp; &nbsp;</th> 
    <th>Name</th>
    <th>Type</th>
    <th>Assigned to</th>
    <th>Image</th>
  </tr>

   
   <% if @assets!= nil %>
   <% @assets.each do |asset| %>
   <tr>
	  <td><%= link_to asset.id,:controller => "asset", :action => "view", :id => asset.id -%></td>
    <td><%= asset.name %></td>
    <td><%= asset.types %></td>
    <td><%= link_to asset.employee_id,:controller => "employee", :action => "profile", :id =>asset.employee_id -%></td>
    <td><%= image_tag asset.image_url %></td>
    <td><%= link_to 'Edit', {:controller => 'asset', :action => 'edit', :id => asset.id} %>
  </tr>
  <% end %>

  <% end %>

</table>



